@model Nge.Web.Models.PlayViewModels.IndexViewModel
@{
    ViewData["Title"] = "Index";
}

<h2>Ввод кодов</h2>



<form asp-action="EnterCode" method="post">
    <div class="row">
        <div class="col-md-4">
            <input asp-for="CodeToEnter" class="form-control" />
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">
            <button type="submit" class="btn btn-default">Ввести</button>
        </div>
    </div>
</form>
